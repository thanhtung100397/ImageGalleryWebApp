{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "types": [
      "jquery",
      "materialize-css"
    ],
    "typeRoots": [
      "node_modules/@types",
      "jquery",
      "materialize-css"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "exclude": [
      "test.ts",
      "**/*.spec.ts"
    ]
  }
}
